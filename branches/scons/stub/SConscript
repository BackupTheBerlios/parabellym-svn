import os
Import('env', 'fnd')

envstub = env.Copy()
envstub.Append(LIBPATH = '#/core', LIBS = 'parabellym')

objs = []

for item in fnd.listFiles(['console', '../util'], r'^.*(\.cc)$'):
	fname, ext = os.path.splitext(item)
	objs.append(envstub.SharedObject(fname + '.stub.o', item))

envstub.Program('pconsole', objs)

